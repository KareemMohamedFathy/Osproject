package Os;

public class process1 extends Simulator implements Runnable{

	@Override
	public void run() {
        try{
        	System.out.println("Process 1:");
        	System.out.println("Please, Enter the filename with the extension. (i.e. 'OS.txt')");
        	PrintFile(TakeInput());
        } 
        catch (Exception e) { 
            System.out.println ("File Not Found"); 
        } 
    } 


	
}
